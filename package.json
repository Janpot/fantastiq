{
  "name": "fantastiq",
  "version": "2.0.0",
  "description": "Job queue implementation on top of Redis",
  "main": "lib",
  "scripts": {
    "test": "npm run lint && npm run mocha",
    "lint": "semistandard --verbose | snazzy",
    "mocha": "mocha ./test",
    "watch:mocha": "npm run mocha -- --watch",
    "toc": "doctoc --notitle ./README.md",
    "prepublish": "npm run build",
    "build": "browserify -t [ babelify --presets [ @babel/preset-env ] ] lib/ui/main.js -o lib/ui/bundle.js",
    "watch:build": "watchify -t [ babelify --presets [ @babel/preset-env ] ] lib/ui/main.js -o lib/ui/bundle.js --verbose"
  },
  "bin": {
    "fantastiq": "bin/fantastiq.js"
  },
  "keywords": [
    "redis",
    "queue",
    "worker",
    "job",
    "promises",
    "promise"
  ],
  "author": "Jan Potoms",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "doctoc": "^1.4.0",
    "faker": "^4.1.0",
    "mocha": "^6.0.2",
    "redis": "^2.8.0",
    "semistandard": "^13.0.1",
    "sinon": "^7.3.1",
    "snazzy": "^8.0.0",
    "supertest": "^4.0.2",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "angular": "^1.7.8",
    "angular-animate": "^1.7.8",
    "angular-aria": "^1.7.8",
    "angular-material": "^1.1.17",
    "angular-messages": "^1.7.8",
    "angular-route": "^1.7.8",
    "angular-sanitize": "^1.7.8",
    "batch-stream": "^0.1.3",
    "bluebird": "^3.5.4",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "extend": "^3.0.2",
    "highcharts": "^7.1.0",
    "join-stream": "0.0.0",
    "passkey": "^3.0.0",
    "readable-stream": "^2.3.6",
    "redis-info": "^3.0.7",
    "split": "^1.0.1",
    "then-redis-scripts": "^2.2.0",
    "yargs": "^11.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/janpot/fantastiq"
  },
  "config": {
    "nyc": {
      "exclude": [
        "node_modules/",
        "test",
        "lib/ui",
        "examples"
      ]
    }
  }
}
