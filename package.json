{
  "name": "fantastiq",
  "version": "1.3.0",
  "description": "Job queue implementation on top of Redis",
  "main": "lib",
  "scripts": {
    "test": "mocha ./test/.setupBabel.js ./test",
    "test-b2d": "REDIS_HOST=192.168.99.100 npm test",
    "toc": "doctoc --notitle ./README.md",
    "lint": "eslint --ignore-pattern ./lib/app/bundle.js .",
    "travis-test": "npm run lint && npm test",
    "prepublish": "npm run build",
    "build": "browserify lib/app/main.js -o lib/app/bundle.js",
    "watch:build": "watchify lib/app/main.js -o lib/app/bundle.js --verbose"
  },
  "bin": {
    "fantastiq": "bin/fantastiq.js"
  },
  "keywords": [
    "redis",
    "queue",
    "worker",
    "job",
    "promises",
    "promise"
  ],
  "author": "Jan Potoms",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-eslint": "^3.1.19",
    "browserify": "^12.0.1",
    "chai": "^3.0.0",
    "doctoc": "^0.14.0",
    "eslint": "^0.24.0",
    "faker": "^2.1.3",
    "mocha": "^2.2.5",
    "redis": "^2.2.5",
    "sinon": "^1.14.1",
    "supertest": "^1.1.0",
    "supertest-as-promised": "^2.0.1",
    "watchify": "^3.6.0"
  },
  "dependencies": {
    "batch-stream": "^0.1.2",
    "bluebird": "^3.0.1",
    "body-parser": "^1.12.4",
    "express": "^4.12.3",
    "extend": "^3.0.0",
    "join-stream": "0.0.0",
    "passkey": "^2.0.0",
    "readable-stream": "^2.0.4",
    "redis-info": "^3.0.6",
    "split": "^1.0.0",
    "then-redis-scripts": "^2.0.0",
    "yargs": "^3.29.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/janpot/fantastiq"
  }
}
