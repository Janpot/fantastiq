{
  "name": "fantastiq",
  "version": "1.5.3",
  "description": "Job queue implementation on top of Redis",
  "main": "lib",
  "scripts": {
    "test": "npm run lint && npm run mocha",
    "lint": "semistandard --verbose | snazzy",
    "mocha": "mocha -r babel-polyfill -r babel-register ./test",
    "watch:mocha": "mocha -w -r babel-polyfill -r babel-register ./test",
    "nyc": "nyc --all npm run mocha",
    "nyc-tmp": "nyc --require babel-polyfill --require babel-register mocha ./test",
    "test-b2d": "REDIS_HOST=192.168.99.100 npm test",
    "toc": "doctoc --notitle ./README.md",
    "prepublish": "npm run build",
    "build": "browserify -t [ babelify --presets [ es2015 ] ] lib/ui/main.js -o lib/ui/bundle.js",
    "watch:build": "watchify -t [ babelify --presets [ es2015 ] ] lib/ui/main.js -o lib/ui/bundle.js --verbose"
  },
  "bin": {
    "fantastiq": "bin/fantastiq.js"
  },
  "keywords": [
    "redis",
    "queue",
    "worker",
    "job",
    "promises",
    "promise"
  ],
  "author": "Jan Potoms",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^5.0.4",
    "babel-polyfill": "^6.1.19",
    "babel-preset-es2015": "^6.1.18",
    "babel-register": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.0.0",
    "doctoc": "^1.0.0",
    "faker": "^3.0.1",
    "mocha": "^2.2.5",
    "nyc": "^6.4.0",
    "redis": "^2.4.2",
    "semistandard": "^7.0.2",
    "sinon": "^1.14.1",
    "snazzy": "^3.0.1",
    "supertest": "^1.1.0",
    "supertest-as-promised": "^3.0.1",
    "watchify": "^3.6.0"
  },
  "dependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.0",
    "angular-aria": "^1.5.0",
    "angular-material": "^1.0.1",
    "angular-messages": "^1.5.3",
    "angular-route": "^1.5.0",
    "angular-sanitize": "^1.5.0",
    "batch-stream": "^0.1.2",
    "bluebird": "^3.0.1",
    "body-parser": "^1.12.4",
    "express": "^4.12.3",
    "extend": "^3.0.0",
    "highcharts": "^4.2.0",
    "join-stream": "0.0.0",
    "passkey": "^3.0.0",
    "readable-stream": "^2.0.4",
    "redis-info": "^3.0.6",
    "split": "^1.0.0",
    "then-redis-scripts": "^2.2.0",
    "yargs": "^3.29.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/janpot/fantastiq"
  },
  "semistandard": {
    "parser": "babel-eslint"
  },
  "config": {
    "nyc": {
      "exclude": [
        "node_modules/",
        "test",
        "lib/ui",
        "examples"
      ]
    }
  }
}
