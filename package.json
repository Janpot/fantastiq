{
  "name": "fantastiq",
  "version": "1.4.5",
  "description": "Job queue implementation on top of Redis",
  "main": "lib",
  "scripts": {
    "test": "npm run lint && npm run mocha",
    "lint": "eslint --ignore-pattern ./lib/app/bundle.js .",
    "mocha": "mocha -r babel-polyfill -r babel-register ./test",
    "nyc": "nyc --all npm run mocha",
    "test-b2d": "REDIS_HOST=192.168.99.100 npm test",
    "toc": "doctoc --notitle ./README.md",
    "prepublish": "npm run build",
    "build": "browserify lib/app/main.js -o lib/app/bundle.js",
    "watch:build": "watchify lib/app/main.js -o lib/app/bundle.js --verbose"
  },
  "bin": {
    "fantastiq": "bin/fantastiq.js"
  },
  "keywords": [
    "redis",
    "queue",
    "worker",
    "job",
    "promises",
    "promise"
  ],
  "author": "Jan Potoms",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^4.1.5",
    "babel-polyfill": "^6.1.19",
    "babel-preset-es2015": "^6.1.18",
    "babel-register": "^6.1.18",
    "browserify": "^12.0.1",
    "chai": "^3.0.0",
    "doctoc": "^0.15.0",
    "eslint": "^1.9.0",
    "faker": "^3.0.1",
    "mocha": "^2.2.5",
    "nyc": "^4.0.0",
    "redis": "^2.4.2",
    "semistandard": "^7.0.2",
    "sinon": "^1.14.1",
    "snazzy": "^2.0.1",
    "supertest": "^1.1.0",
    "supertest-as-promised": "^2.0.1",
    "watchify": "^3.6.0"
  },
  "dependencies": {
    "batch-stream": "^0.1.2",
    "bluebird": "^3.0.1",
    "body-parser": "^1.12.4",
    "express": "^4.12.3",
    "extend": "^3.0.0",
    "join-stream": "0.0.0",
    "passkey": "^2.0.0",
    "readable-stream": "^2.0.4",
    "redis-info": "^3.0.6",
    "split": "^1.0.0",
    "then-redis-scripts": "^2.2.0",
    "yargs": "^3.29.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/janpot/fantastiq"
  },
  "config": {
    "nyc": {
      "exclude": [
        "node_modules/",
        "test",
        "lib/app",
        "examples"
      ]
    }
  }
}
