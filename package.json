{
  "name": "fantastiq",
  "version": "1.5.4",
  "description": "Job queue implementation on top of Redis",
  "main": "lib",
  "scripts": {
    "test": "npm run lint && npm run mocha",
    "lint": "semistandard --verbose | snazzy",
    "mocha": "mocha ./test",
    "watch:mocha": "npm run mocha -- --watch",
    "nyc": "nyc --all npm run mocha",
    "toc": "doctoc --notitle ./README.md",
    "prepublish": "npm run build",
    "build": "browserify -t [ babelify --presets [ env ] ] lib/ui/main.js -o lib/ui/bundle.js",
    "watch:build": "watchify -t [ babelify --presets [ env ] ] lib/ui/main.js -o lib/ui/bundle.js --verbose"
  },
  "bin": {
    "fantastiq": "bin/fantastiq.js"
  },
  "keywords": [
    "redis",
    "queue",
    "worker",
    "job",
    "promises",
    "promise"
  ],
  "author": "Jan Potoms",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "chai": "^4.1.2",
    "doctoc": "^1.3.1",
    "faker": "^4.1.0",
    "mocha": "^5.2.0",
    "nyc": "^11.8.0",
    "redis": "^2.8.0",
    "semistandard": "^12.0.0",
    "sinon": "^5.0.10",
    "snazzy": "^7.1.1",
    "supertest": "^3.1.0",
    "watchify": "^3.11.0"
  },
  "dependencies": {
    "angular": "^1.7.0",
    "angular-animate": "^1.7.0",
    "angular-aria": "^1.7.0",
    "angular-material": "^1.1.9",
    "angular-messages": "^1.7.0",
    "angular-route": "^1.7.0",
    "angular-sanitize": "^1.7.0",
    "batch-stream": "^0.1.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "extend": "^3.0.1",
    "highcharts": "^6.1.0",
    "join-stream": "0.0.0",
    "passkey": "^3.0.0",
    "readable-stream": "^2.3.6",
    "redis-info": "^3.0.6",
    "split": "^1.0.1",
    "then-redis-scripts": "^2.2.0",
    "yargs": "^11.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/janpot/fantastiq"
  },
  "config": {
    "nyc": {
      "exclude": [
        "node_modules/",
        "test",
        "lib/ui",
        "examples"
      ]
    }
  }
}
